        <!--<obsIds>001 002 004 008 015 033</obsIds>-->
        <voltage>3.3</voltage>
        <embeddedImageId>LWBTest</embeddedImageId>
    </targetConf>
        
    <!-- Serial Service configuration -->
    <serialConf>
        <obsIds>001 002 003 004 006 008 015 016 018 022 024 027 028 031 032 033 200 202 201 204</obsIds>
        <port>serial</port>
        <baudrate>115200</baudrate>
        <mode>ascii</mode>
        <remoteIp>255.255.255.255</remoteIp>
    </serialConf>

    <!-- GPIO Tracing Service configuration -->
    <gpioTracingConf>
        <obsIds>001 002 003 004 006 008 015 016 018 022 024 027 028 031 032 033 200 202 201 204</obsIds>
        <pinConf>
            <pin>INT1</pin>
            <edge>both</edge>
            <mode>continuous</mode>
        </pinConf>
        <pinConf>
            <pin>INT2</pin>
            <edge>both</edge>
            <mode>continuous</mode>
        </pinConf>
        <pinConf>
            <pin>LED1</pin>
            <edge>both</edge>
            <mode>continuous</mode>
        </pinConf>
        <pinConf>
            <pin>LED2</pin>
            <edge>both</edge>
            <mode>continuous</mode>
        </pinConf>
        <pinConf>
            <pin>LED3</pin>
            <edge>both</edge>
            <mode>continuous</mode>
        </pinConf>
    </gpioTracingConf>

