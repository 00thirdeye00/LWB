    <!-- General configuration -->
    <generalConf>
        <name>LWB Test</name>
        <description>Low-Power Wireless Bus Test (static scheduler)</description>
        <scheduleAsap>
            <durationSecs>900</durationSecs>
        </scheduleAsap>
<!-- 
        <scheduleAbsolute>
            <start>2014-03-14T06:00:00Z</start>
            <end>2014-03-14T07:00:00Z</end>
        </scheduleAbsolute>
 -->
        <emailResults>no</emailResults>
    </generalConf>
    
    <!-- Target configuration -->
    <targetConf>
        <obsIds>001 002 003 004 006 008 015 016 018 022 024 027 028 031 032 033 200 202 201 204</obsIds>
        <!--<obsIds>001 002 003 004 006 007 008 010 011 013 014 015 016 017 018 019 020 022 023 024 025 026 027 028 031 032 033 200 201 202 204</obsIds>-->
